# Environment Variables for Customer Mapping App
# Copy this file to .env and set your values

# Google Maps API Key (Required)
# Get your API key from: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Session Security (Required for production)
SESSION_SECRET=change-this-to-a-random-string-for-production

# Port (Optional - platforms will set this automatically)
PORT=3001

# Node Environment
NODE_ENV=production

# Authentication Credentials (Required)
AUTH_USERNAME=your-username-here
AUTH_PASSWORD=your-password-here